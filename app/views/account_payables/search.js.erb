 <% if @account_payables.present? %>
   $('#results').empty();
   $("#results").append("<%= j( render 'search', account_payables: @account_payables )  %>");
 <% else %>
   $('#results').empty();
   $("#results").append("<%= j(content_tag(:div, content_tag(:p, 'Not found any accounts payable'), class: 'alert alert-danger')) %> ");
 <% end %>
